OPERATOR(PLUS,  "+",   _ADD(dL, dR))
OPERATOR(MINUS, "-",   _MINUS(dL, dR))
OPERATOR(MUL,   "*",   _ADD(_MUL(dL, cR), _MUL(cL, dR)))
OPERATOR(DIV,   "/",   _DIV(_MINUS(_MUL(dL, cR), _MUL(cL, dR)), _MUL(cR, cR)))
OPERATOR(SIN,   "sin", _MUL(_COS(cL), dL))
OPERATOR(COS,   "cos", _MUL(_NEGATIVE, _MUL(_SIN(cL), dL)))
OPERATOR(EXP,   "exp", _MUL(_EXP(cL), dL))
OPERATOR(LN,    "ln",  _DIV(dL, cL))
OPERATOR(TAN,   "tan", _DIV(dL, _MUL(_COS(cL), _COS(cL))))
